    <script>
		$(document).ready(function() {
			<!-- news counter -->
 			$('#newscounter').newscounter({
    			newsId : '201607061770895',
    			counterUrl : 'http://news.cts.com.tw//action/news_count.php'
    		});
			//大圖輪播
			 $("#owl-slide").owlCarousel({
				slideSpeed : 300,
				singleItem:true,
				lazyLoad : true,
				autoPlay : 3000,
			    stopOnHover : true,
				lazyLoad : true, 
				pagination : true,
				navigation: true,
				navigationText: [
				  "<i class='icon-chevron-left icon-white'></i>",
				  "<i class='icon-chevron-right icon-white'></i>"
				  ]
	
			  });
				
			
			//小圖輪播	
			  var owl = $("#owl-focus");
			//Sort random function
			  function random(owlSelector){
				owlSelector.children().sort(function(){
					return Math.round(Math.random()) - 0.5;
				}).each(function(){
				  $(this).appendTo(owlSelector);
				});
			  }
			 
			  owl.owlCarousel({
				items : 4, //10 items above 1000px browser width
				itemsDesktop : [1000,4], //5 items between 1000px and 901px
				itemsDesktopSmall : [900,3], // betweem 900px and 601px
				itemsTablet: [600,2], //2 items between 600 and 0
				itemsMobile : 1 ,// itemsMobile disabled - inherit from itemsTablet option 
				lazyLoad : true,
				pagination : false,
				navigation: true,
				navigationText: [
				  "<i class='icon-chevron-left icon-white'></i>",
				  "<i class='icon-chevron-right icon-white'></i>"
				  ],
				beforeInit : function(elem){
				  //Parameter elem pointing to $("#owl-demo")
				  random(elem);
				}
			 
			  });
		  
		  
		  
		  //fix topbar
		    var flag = false;
                $(window).scroll(function() {
                   
                  if (flag == false) {
                      flag = true;
                      setTimeout(loadMore,200);
                  }
                });
           
            function loadMore(){
              if ( $(window).scrollTop() > 75){
                  $(".top_sub_nav").css({"position":"fixed","top":0,"z-index":"9999","box-shadow":"0 0 15px #999"}); 
              } else {
                 $(".top_sub_nav").css({"position":"","top":"","z-index":"0","box-shadow":""}); 
              }
              flag = false;
            }
			
			
		});
	</script>
