bling.previousSibling,\r\n                            handler = function() {\r\n                            var styles = window.getComputedStyle(scupio_iframe_sibling),\r\n                            hor_padding = parseFloat(styles.paddingBottom)+parseFloat(styles.paddingTop),\r\n                            current_height = scupio_iframe_sibling.offsetHeight - hor_padding + 'px';\r\n                            if (scupio_iframe.style.height !== current_height) {\r\n                            scupio_iframe.style.height=current_height;}\r\n                            };\r\n                            handler();\r\n                            window.addEventListener('resize', handler);\r\n                            }());\r\n                            </script>\r\n                            \r\n                    <style>\r\n\t\t\t\t\t#news_list_block .single_news:nth-child(2),nth-child(n+4):nth-child(odd) {\r\n\t\t\t\t\t\tbackground-color: #EDF8F9;\r\n\t\t\t\t\t}\r\n\t\t\t\t\t</style>\r\n            \r\n                                <div class=\"single_news block100 bottom_dot clrboth\">\r\n                                <div class=\"news_smimg\">\r\n                    <a href=\"http://news.cts.com.tw/cts/campus/201606/201606291768397.html\" ><img src=\"http://news.cts.com.tw/photo/cts/201606/201606291768397_wm.jpg\"  title=\"\u4fe1\u7fa9\u570b\u5c0f \u8a2d\u8a08\u5bb6\u805a\u843d\u793a\u7bc4\u5b78\u6821\" border=\"0\" /></a>\r\n                </div>\r\n                                                <div class=\"news_right\">\r\n                                <a href=\"http://news.cts.com.tw/cts/campus/201606/201606291768397.html\" title=\"\u4fe1\u7fa9\u570b\u5c0f \u8a2d\u8a08\u5bb6\u805a\u843d\u793a\u7bc4\u5b78\u6821\" >\r\n                    <div class=\"mainnewstitle\">\u4fe1\u7fa9\u570b\u5c0f \u8a2d\u8a08\u5bb6\u805a\u843d\u793a\u7bc4\u5b78\u6821</div>\r\n                    <div id=\"9\" class=\"date newstitle\" style=\"float:right;\">\u3010\u83ef\u899