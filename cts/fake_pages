          function loadMore(){\r\n              if ( $(window).scrollTop() > 75){\r\n                  $(\".top_sub_nav\").css({\"position\":\"fixed\",\"top\":0,\"z-index\":\"9999\",\"box-shadow\":\"0 0 15px #999\"}); \r\n              } else {\r\n                 $(\".top_sub_nav\").css({\"position\":\"\",\"top\":\"\",\"z-index\":\"0\",\"box-shadow\":\"\"}); \r\n              }\r\n              flag = false;\r\n            }\r\n\t\t\t\r\n\t\t\t\r\n\t\t});\r\n\t</script>\r\n <!--footer-->\r\n    \r\n<div class=\"block100 boder_box white_box footer_section\">\r\n\t\r\n        <div class=\"AD_970_90\">\r\n        <div id='div-gpt-ad-1403166194349-0' style='width:970px; height:90px;'>\r\n        </div>\r\n        <div id='div-gpt-ad-1403166194349-1' style='width:320px;'>\r\n        </div>\r\n    </div>\r\n\r\n\r\n    <div class=\"clrboth block100\" style=\"height:10px;\"></div>\r\n\t<!--start sitemap-->\r\n    <div class=\"block100 footer_sitemap\">\r\n\t<div class=\"title_bar\"><h3> \u7db2\u7ad9\u5730\u5716 </h3></div>\r\n    <div class=\"block25  right_line\">\r\n    \t<div class=\"title_bar subtitle_bar\">\u65b0\u3000\u805e</div>\r\n        <div class=\"block50\">\r\n        \t<ul>\r\n            \t<li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/real/\">\u5373\u6642\u65b0\u805e</a></span></li>\r\n            \t<li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/politics/\">\u653f\u6cbb\u65b0\u805e</a></span></li>\r\n                <li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/society/\">\u793e\u6703\u65b0\u805e</a></span></li>\r\n                <li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/money/\">\u8ca1\u7d93\u65b0\u805e</a></span></li>\r\n\t\t\t\t<li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/entertain/\">\u5a1b\u6a02\u65b0\u805e</a></span></li>\r\n                <li><span class=\"progcontents\"><a href=\"http://news.cts.com.tw/international/\">\u570b\u969b\u65b0\u805e</