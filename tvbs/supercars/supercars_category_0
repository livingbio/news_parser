		var icon_url ="http://cc.tvbs.com.tw/portal/images/weather_icon/";
		var click_data =city_data[city_name];
		
		var date = new Date();
		var mem_cookie_day = 7;
			 date.setTime(date.getTime() + (mem_cookie_day *24 * 60*60 * 1000));
		  	 $.cookie("cho_weather", city_name,{ path: '/' ,domain:'' ,expires: date});

		var 	weather_html ='<div class="header-top-R-weather-img" id="weather"><img src="'+icon_url+click_data[0].parameterValue+'.gif"></div>'+
                    '<div class="header-top-R-weather-temperature">'+click_data[0].min_t+'°C</div>'+
                    '<div class="header-top-R-weather-city">'+click_data.city+'</div> ';
			
		$("#weather_div").html(weather_html);
		$("#region_pm2").html(click_data.pm2_5);
		$("#region_title").html(city_name);
		
		var week_weather_html = "";
		
		for(var i=0;i<5;i++)
		{
			if(i==0)
			{
				week_weather_html= '<div class="weather_today">'+
						 '<div class="weather_week today">'+click_data[i].time+'<p>'+click_data[i].weekday+'</p></div>'+
						 '<div class="weather_icon today"><img src="'+icon_url+click_data[i].parameterValue+'.gif"></div>'+
						 '<div class="weather_temperature today">'+click_data[i].min_t+'°C-'+click_data[i].max_t+'°C</div>'+
						 '</div>';
			}
			else
			{
				week_weather_html += '<div class="weather_day">'+
						 '<div class="weather_week">'+click_data[i].time+'<p>'+click_data[i].weekday+'</p></div>'+
						 '<div class="weather_icon"><img src="'+icon_url+click_data[i].parameterValue+'.gif"></div>'+
						 '<div class="weather_temperature">'+click_data[i].min_t+'°C-'+click_data[i].max_t+'°C</div>'+
						 '</div>';
			}
		}
		
		$("#weather_block").html(week_weather_html);
		
		$('#region_titel_setup_div').hide();
	
	}
	
