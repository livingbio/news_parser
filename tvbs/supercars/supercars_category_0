	<!-- 分類即時新聞 -->
	<script type='text/javascript'>
	$(function(){
		var stop=true;//開啟自動加載
		
		$(window).scroll(function(){
			if(detectmob()){
				if(stop==true){
					if ($(this).scrollTop() + $(window).height()+500 >= $(document).height() && $(this).scrollTop() ) {
						stop=false;//關閉自動加載.待加載完成後才開啟
						$('.more').click();
					}
				}	
			}
		});
		var news_key=12;//即時新聞動態載入啟始編號 (新聞9筆+廣告2筆+1=12)
		var ad_num=0;//載入廣告計數
		
		//取得即時新聞資料
		function getData(addpage,news_id,get_num){
			console.log('http://news.tvbs.com.tw/news/get_cate_news_json/'+news_id+'/supercars/'+addpage);
			
			$.get('http://news.tvbs.com.tw/news/get_cate_news_json/'+news_id+'/supercars/'+addpage,function(data){
			
				if(data=='null'){
					alert('閱讀完畢!');
					return false;
				}
				
				var jdata=JSON.parse(data);//將JSON轉為陣列資料
				var html='';
				
				//即時新聞廣告出現位置
				//var ad_index=Array(4,10,16,22,28);
				
				for(i=0;i<jdata.length;i++){
					
					/*
					//符合廣告出現條件
					for(j=0;j<=ad_index.length;j++){
						if((news_key+ad_num)==ad_index[j]){
							
							//html+="<li class='ad-banner-m'><a href='#'><img src='http://cc.tvbs.com.tw/news/images/header-ad-640x200.jpg'></a></li>";
							//html+="<li class='ad-banner-m'><div id='googleadgoeshere' style='width:100%;height:300px;'></div></li>";
							
							ad_num++;
						}
					}
					*/
					
					html+="<li>";
					html+="<a href='../"+jdata[i]['news_category_label']+'/'+jdata[i]['news_id']+"'>";
					html+="<div class='information-img1'>";
					
					//判斷新聞版本
					if(jdata[i]['news_version']==3){
						html+="<img src='"+jdata[i]['news_img']+"'>";
					}else{
						html+="<img src='"+jdata[i]['news_img']+"'>";
					}
					news_key++;
					
					html+="</div>";
					html+="</a>";
					html+="<div class='information-txt1'>";
										html+="<a href='../"+jdata[i]['news_category_label']+'/'+jdata[i]['news_id']+"'>";
					html+="<p class='titel_content18'>"+jdata[i]['news_title']+"</p>";
					html+="</a>";
					html+="</div>";
					html+="<a href='../"+jdata[i]['news_category_label']+'/'+jdata[i]['news_id']+"'>";
					html+="<div class='information-txt1-under'>";
					html+="<p class='under'>"+jdata[i]['news_published_date']+"</p>";
					html+="<p class='underView'>瀏覽數:"+(jdata[i]['news_pageview']==''||jdata[i]['news_pageview']==null?0:jdata[i]['news_pageview'])+"</p> ";
					html+="</div>";
					html+="</a>";
					html+="</li>";
					if(i==jdata.length-1){
						$('.more').attr('news_id', jdata[i]['news_id']);
					}
				}
				//將html資料貼到li區塊
				$('#addpage li:last-child').after(html);
				stop=true;//開啟自動加載
				 //setTimeout(function(){
					 
								
				$("img.lazyimage").lazyload();
			});
		}  
		
		//more btn
		$('.more').click(function(){
			var addpage=$(this).attr('addpage');
			var news_id=$(this).attr('news_id');
			var get_num=6;//抓取筆數
			if(addpage>5){
				getData(addpage,news_id,get_num);//NEWS MORE
			}else{
				$("#addpage li.hiddenblock:lt(6)").removeClass("hiddenblock");
				if(detectmob()){
					$("#addpage li.hiddenads:first").removeClass("hiddenads");
					stop=true;//開啟自動加載
				}
			}
			var page=parseInt(addpage)+1;//按鈕撈取資料的頁數+1
			
			$(this).attr('addpage',page);
			// $(this).attr('news_id',news_id);
		});
		$("img.lazyimage").lazyload();
	});
	</script>
	
		
