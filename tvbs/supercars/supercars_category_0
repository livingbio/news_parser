<script>

		var flash_news_id = 0;
		var flash_news ="";		
		var flash_news_max =0;
		var flash_cookie_time = 60000;	// 
		var flash_show_time = 5000; //快訊更新時間
		var updata_flash_time = 60000; //更新
		
		
		
		update_get_flash_news(); //frist run
	function update_get_flash_news() //update api data
	{
		$.getJSON( "http://news.tvbs.com.tw/api/get_flash_news/news", function( data ) 
		{
			$(".flash_news_advanced").hide();
			$(".flash_news_basic").hide();
			
			$.cookie('flash_news_close', '', { expires: -1 });
			$.cookie('flash_news_close_basic', '', { expires: -1 });
			
			//alert("更新快訊來源資料");
			var tmp_html;
			flash_news_max = data.flash_news_max;
		
			flash_news = data.flash_news;
			flash_news_id= 0;
			
			if(flash_news_max >= 1)
			{
				if(flash_news[0].show_type == 1)
				{
					$(".flash_news_advanced").show();
					
					featureBox1_html ="<div class='featureBox1_pic'><img src='"+flash_news[0].pic_url+"'></div>"+
					"<div class='featureBox1_txt'>"+
					"<div class='flash_news_icon'>"+flash_news[0].show_type_name+"</div>"+
					"<div class='featureBox1_txt_title'>"+flash_news[0].title+"</div>"+
					"<div class='featureBox1_txt_date'>"+flash_news[0].show_time+"</div>"+
					"<div id='featureBox1_hide_alert_news_type' style='display:none'>"+flash_news[0].blank+"</div>"+
					"<div id='featureBox1_hide_url' style='display:none'>"+flash_news[0].url+"</div>"+
					"</div>";
					$("#featureBox1").html(featureBox1_html);
					
				}
				else
				{
					$(".flash_news_basic").show();
					
					featureBox2_html ="<div class='flash_news_icon basic'>"+flash_news[0].show_type_name+"</div>"+
					"<div class='featureBox2_txt_title'>"+flash_news[0].title+"</div>"+
					"<div class='featureBox2_txt_time'>"+flash_news[0].show_time+"</div>"+
					"<div id='featureBox2_hide_alert_news_type' style='display:none'>"+flash_news[0].blank+"</div>"+
					"<div id='featureBox2_hide_url' style='display:none'>"+flash_news[0].url+"</div>";
					$("#featureBox2").html(featureBox2_html);
				}
			}
			else
				return;

		  

					
			  
			  
		});
	}
	
	$( "#featureBox1" ).click(function() {
		//alert();
		if($("#featureBox1_hide_alert_news_type").html() == "1")
			window.open($("#featureBox1_hide_url").html());
		else
			top.location.href = $("#featureBox1_hide_url").html();
			
	});
 $( ".flash_news_prev_basic" ).click(function() {
	update_flash_news(-1);
});
$( ".flash_news_next_basic" ).click(function() {
	update_flash_news();
});
$( ".flash_news_prev" ).click(function() {
	update_flash_news(-1);
});
$( ".flash_news_next" ).click(function() {
	update_flash_news();
}); 

 	  $(".flash_news_close").on('click',function()
	  {
		//$flash_news_advanced.css({'display': 'none'});
		$(".flash_news_advanced").hide();
		
			var date = new Date();
			 date.setTime(date.getTime() + (flash_cookie_time));
		  	 $.cookie("flash_news_close", "1",{ path: '/' ,domain:'' ,expires: date});
	  });

	  $(".flash_news_close_basic").on('click',function()
	  {
		//$flash_news_basic.css({'display': 'none'});
		$(".flash_news_basic").hide();
		var date = new Date();
			 date.setTime(date.getTime() + (flash_cookie_time));
		  	 $.cookie("flash_news_close_basic", "1",{ path: '/' ,domain:'' ,expires: date});
		
	  }); 


$( "#featureBox2" ).click(function() {
		if($("#featureBox2_hide_alert_news_type").html() == "1")
			window.open($("#featureBox2_hide_url").html());
		else
			top.location.href = $("#featureBox2_hide_url").html();
	});
	
	
	function update_flash_news(num)
	{
		var tmp_i=0;
		num = num || 1; //如果num沒有預設值 則填入 1
		if(flash_news_max <= 1)
			return;
				

		flash_news_id+=num;
		if(flash_news_id <=0 )
		{
			flash_news_id = flash_news_max-1;
		}
				
		 tmp_i = flash_news_id % flash_news_max;		
		if(flash_news[tmp_i].show_type == 1)
		{
			if(!$.cookie("flash_news_close"))
				$(".flash_news_advanced").show();
			else
				$(".flash_news_advanced").hide();
			$(".flash_news_basic").hide();
			featureBox1_html ="<div class='featureBox1_pic'><img src='"+flash_news[tmp_i].pic_url+"'></div>"+
					"<div class='featureBox1_txt'>"+
					"<div class='flash_news_icon'>"+flash_news[tmp_i].show_type_name+"</div>"+
					"<div class='featureBox1_txt_title'>"+flash_news[tmp_i].title+"</div>"+
					"<div class='featureBox1_txt_date'>"+flash_news[tmp_i].show_time+"</div>"+
					"<div id='featureBox1_hide_alert_news_type' style='display:none'>"+flash_news[tmp_i].blank+"</div>"+
					"<div id='featureBox1_hide_url' style='display:none'>"+flash_news[tmp_i].url+"</div>"+
					"</div>";
					$("#featureBox1").html(featureBox1_html);
		  }
		  else
		  {
				$(".flash_news_advanced").hide();
				
			if(!$.cookie("flash_news_close_basic"))
				$(".flash_news_basic").show();
			else
				$(".flash_news_basic").hide();
					featureBox2_html ="<div class='flash_news_icon basic'>"+flash_news[tmp_i].show_type_name+"</div>"+
					"<div class='featureBox2_txt_title'>"+flash_news[tmp_i].title+"</div>"+
					"<div class='featureBox2_txt_time'>"+flash_news[tmp_i].show_time+"</div>"+
					"<div id='featureBox2_hide_alert_news_type' style='display:none'>"+flash_news[tmp_i].blank+"</div>"+
					"<div id='featureBox2_hide_url' style='display:none'>"+flash_news[tmp_i].url+"</div>";
		  
				$("#featureBox2").html(featureBox2_html);
		  } 


					
			  
			 
		

	}
	setInterval(update_flash_news,flash_show_time); //3秒 更新快訊
	setInterval(update_get_flash_news,updata_flash_time); //10分鐘 更新json 來源 

</script>
