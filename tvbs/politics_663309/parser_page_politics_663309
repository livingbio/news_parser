		<script type='text/javascript'>
		$(function(){
			//新聞內容 我是圖說
			var add_img_alt_before='';
			var add_img_alt_after='';
			//尋找新聞內容圖片有ALT
			$('.newsdetail-content img').each(function(i, obj) {
				if($(obj).attr('alt')!==undefined && $(obj).attr('alt')!==''){
					//圖片增加圖說Layout
					//add_img_alt_before="<div class='newsdetail-img'><div class='newsdetail-img-magnify'></div>";
					add_img_alt_before="<div class='newsdetail-img'></div>";
					add_img_alt_after="</div><div class='newsdetail-img-t'><p class='color3'>"+$(obj).attr('alt')+"</p><div class='newsdetail-association'></div></div>";
					$(obj).before(add_img_alt_before);
					$(obj).after(add_img_alt_after);
				}
			});
			
			$.get('http://news.tvbs.com.tw/news/newsprev/663309/7', function(data){
				if(data=='null'||data==''){
					$('#prevLinkButton').hide();
					$('#prevMobileLink').hide();
				}else{
					var result = JSON.parse(data);
					// console.log(result);
					$('#prevLinkButton').attr('href', result.news_id);
					$('#prevLinkImg').attr('src', result.news_img);
					$('#prevLinkP').text(result.news_title);
					$('#prevMobileLink').attr('href', result.news_id);
					$('#prevMobileP').text(result.news_title);
				}
			});
			
			$.get('http://news.tvbs.com.tw/news/newsnext/663309/7', function(data){
				if(data=='null'||data==''){
					$('#nextLinkButton').hide();
					$('#nextMobileLink').hide();
				}else{
					var result = JSON.parse(data);
					// console.log(result);
					$('#nextLinkButton').attr('href', result.news_id);
					$('#nextLinkImg').attr('src', result.news_img);
					$('#nextLinkP').text(result.news_title);
					$('#nextMobileLink').attr('href', result.news_id);
					$('#nextMobileP').text(result.news_title);
				}
			});
			
			// $('ul#addpage').hide();
			$.get('http://news.tvbs.com.tw/news/newscatemore/7', function(data){
				var result = JSON.parse(data);
				// console.log(result);
				var ul = $('ul#addpage');
				
				for (var prop in result) {
					if (result.hasOwnProperty(prop)) {
						var i = Object.keys(result).indexOf(prop);
						//如果舊新聞沒有歸屬分類，網址則補上other的分類
						var link_label=(result[prop].news_category_label!='')?result[prop].news_category_label:'other';
						var link="/"+link_label+"/"+result[prop].news_id;
						// console.log(result[prop]);
						var li = ul.find('li:first').clone(true).appendTo(ul);
						var news_youtube = result[prop].news_youtube;
						var news_img = result[prop].news_img;
						var news_title = result[prop].news_title;
						var news_published_date = result[prop].news_published_date;
						var news_pageview = result[prop].news_pageview;
						if(news_youtube==''){
							$('.cateMoreYoutube:eq('+i+')').hide();
						}
						$('.cateMoreFLink:eq('+i+')').attr('href', link);
						$('.cateMoreSLink:eq('+i+')').attr('href', link);
						$('.cateMoreTLink:eq('+i+')').attr('href', link);
						$('.cateMoreImage:eq('+i+')').attr('data-original', news_img);
						$('.cateMoreTitle:eq('+i+')').text(news_title);
						$('.cateMoreUnder:eq('+i+')').text(news_published_date);
						$('.cateMoreUnderView:eq('+i+')').text('瀏覽數:'+news_pageview);
					}
				}
				$("img.cateMoreImage").lazyload();
				ul.find('li:last').remove();
				ul.find('li').show();
				ul.show();
			});
			
			$("img.lazyimage").lazyload();
		});
	</script>
	
